      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
