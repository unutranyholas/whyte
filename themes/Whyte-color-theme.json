  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#657B83"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Variable start",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Support.construct",
      "scope": ["support.function.construct", "keyword.other.new"],
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "fontStyle": "",
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "fontStyle": "",
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#2AA198"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316bcd"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "name": "JSX, HTML markup",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "support.class.component",
      ],
      "settings": {
        "foreground": "#f17a0b",
        "fontStyle": ""
      }
    },
    {
      "name": "Unimportant stuff",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.separator.comma",
        "punctuation.terminator.statement",
        "punctuation.separator.parameter",
        "meta.brace.round",
        "meta.block",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.binding-pattern.object",
        "punctuation.separator.key-value",
        "meta.class",
        "punctuation.accessor",
        "punctuation.destructuring",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.array.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json",
        "punctuation.separator.dictionary.key-value.jsonc",
        "punctuation.separator.array.jsonc",
        "punctuation.separator.dictionary.pair.jsonc",
        "punctuation.support.type.property-name.begin.jsonc",
        "punctuation.support.type.property-name.end.jsonc"
      ],
      "settings": {
        "foreground": "#434d6852",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.definition.block",
      "settings": {}
    },
    {
      "name": "Brackets I'd like to highlight",
      "scope": [
        "meta.brace.square",
        "meta.objectliteral",
        "punctuation.definition.binding-pattern.object"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Functions",
      "scope": [
        "support.function",
        "entity.name.function",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#0087D1",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other",
        "support.class",
        "variable.parameter",
        "variable.object.property",
        "entity.other.inherited-class",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "#ac3059",
        "fontStyle": ""
      }
    },
    {
      "name": "System stuff I don't understand",
      "scope": [
        "support.constant",
        "constant.language",
        "variable.language",
        "entity.name.type",
        "support.variable.dom",
        "support.class.console",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#BD8E00",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": {}
    },
    {
      "name": "Math stuff",
      "scope": [
        "constant.numeric",
        "keyword.operator.relational",
        "keyword.operator.arithmetic",
        "meta.delimiter.decimal.period"
      ],
      "settings": {
        "foreground": "#9125da",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string.template",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "punctuation.definition.string.begin.jsonc",
        "punctuation.definition.string.end.jsonc"
      ],
      "settings": {
        "foreground": "#599c3a",
        "fontStyle": ""
      }
    },
    {
      "name": "Must be black: property names, reserved words etc",
      "scope": [
        "entity.other.attribute-name",
        "variable",
        "support",
        "storage",
        "meta",
        "keyword",
        "entity",
        "constant",
        "emphasis",
        "meta.tag",
        "entity.name.tag.yaml",
        "punctuation.definition.block.sequence.item.yaml",
        "support.type.property-name.json",
        "support.type.property-name.jsonc",
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "storage.type.class",
        "keyword.operator.logical",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.section.markdown",
        "markup.heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "beginning.punctuation.definition.list.markdown",
        "meta.separator.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown",
        "markup.heading.setext",
        "beginning.punctuation.definition.quote.markdown"
      ],
      "settings": {
        "foreground": "#00000044",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#599c3a",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.italic",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.description.markdown",
        "string.other.link.title.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#295AE0",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["constant.character"],
      "settings": {
        "foreground": "#f17a0b"
      }
    },
    {
      "name": "Must be black",
      "scope": ["variable.object.property"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Unimportant stuff",
      "scope": [
        "keyword.operator.type.annotation",
        "punctuation.definition.typeparameters.begin",
        "punctuation.definition.typeparameters.end",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#434d6852",
        "fontStyle": ""
      }
    },
    {
      "name": "System stuff I don't understand",
      "scope": ["support.type.primitive", "comment"],
      "settings": {
        "foreground": "#37857b98",
        "fontStyle": ""
      }
    },
    {
      "name": "System stuff I don't understand",
      "scope": ["support.type.builtin.ts"],
      "settings": {
        "foreground": "#BD8E00",
        "fontStyle": ""
